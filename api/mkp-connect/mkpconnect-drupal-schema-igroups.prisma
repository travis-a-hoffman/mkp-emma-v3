  nid       Int    @id @default(autoincrement()) @mkp_connect_drupal_db.UnsignedInt
  vid       Int?   @unique(map: "vid") @mkp_connect_drupal_db.UnsignedInt
  type      String @default("") @mkp_connect_drupal_db.VarChar(32)
  language  String @default("") @mkp_connect_drupal_db.VarChar(12)
  title     String @default("") @mkp_connect_drupal_db.VarChar(255)
  uid       Int    @default(0)
  status    Int    @default(1)
  created   Int    @default(0)
  changed   Int    @default(0)
  comment   Int    @default(0)
  promote   Int    @default(0)
  sticky    Int    @default(0)
  tnid      Int    @default(0) @mkp_connect_drupal_db.UnsignedInt
  translate Int    @default(0)

  iGroupClass                IGroupClassField?
  iGroupStatus               IGroupStatusField?
  iGroupType                 IGroupTypeField?
  owner                      IGroupOwnerField?
  format                     IGroupFormatField?
  meetingNight               IGroupMeetingNightField?
  meetingTime                IGroupMeetingTimeField?
  meetingFrequency           IGroupFrequencyField?
  areaName                   IGroupAreaField?
  communityName              IGroupCommunityField?
  city                       IGroupCityField?
  stateProvince              IGroupStateProvinceField?
  postalCode                 IGroupPostalCodeField?
  latitude                   IGroupLatitudeField?
  longitude                  IGroupLongitudeField?
  acceptingNewMembers        IGroupAcceptingNewMembersField?
  acceptsInitiatedVisitors   IGroupAcceptsInitiatedVisitorsField?
  acceptsUninitiatedVisitors IGroupAcceptsUninitiatedVisitorsField?
  publicDisplay              IGroupPublicDisplayField?
  mixedGender                IGroupMixedGenderField?
  contactUid                 IGroupContactUidField?
  //contact_name               civicrm_contact // Exists in a different schema

  @@index([language], map: "language")
  @@index([changed], map: "node_changed")
  @@index([created], map: "node_created")
  @@index([promote, status, sticky, created], map: "node_frontpage")
  @@index([status, type, nid], map: "node_status_type")
  @@index([title, type(length: 4)], map: "node_title_type")
  @@index([type(length: 4)], map: "node_type")
  @@index([tnid], map: "tnid")
  @@index([translate], map: "translate")
  @@index([uid], map: "uid")
  @@map("node")
