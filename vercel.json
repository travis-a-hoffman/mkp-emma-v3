{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm --ignore-run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "cleanUrls": false,
  "trailingSlash": false
}
